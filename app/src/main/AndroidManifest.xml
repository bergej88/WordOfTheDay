<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.drunkcollegeapps.jeff.wordoftheday">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".WOTDApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- Main Activity -->

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"></activity>

        <!-- Login Activity -->

        <activity
            android:name=".LoginGoogleActivity"
            android:label="@string/app_name"
            android:theme="@style/ThemeOverlay.MyNoTitleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
     		App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".TestLandingActivity"></activity>
    </application>

</manifest>